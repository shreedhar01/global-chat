{
  "name": "chatapp",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"green,blue\" \"pnpm --filter backend dev\" \"pnpm --filter frontend dev\"",
    "dev:backend": "pnpm --filter backend dev",
    "dev:frontend": "pnpm --filter frontend dev",
    "prepare": "husky",
    "lint": "concurrently -n \"BACKEND,FRONTEND\" -c \"green,blue\" \"pnpm --filter backend lint\" \"pnpm --filter frontend lint\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/format": "^20.0.0",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6"
  }
}
